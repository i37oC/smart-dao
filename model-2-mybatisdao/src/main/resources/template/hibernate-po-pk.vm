		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		final ${outputor.className}PK other = (${outputor.className}PK) obj;

		#foreach($column in $outputor.columnPKs)
			if (${column.fieldName} == null) {
